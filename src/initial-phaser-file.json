  {
      "l":["clsStopwatch"],
      "relation":"instance_of",
      "r":["function"],
      "params":[],
      "lines":[
	  "//console.log('CREATED STOPWATC')",
	  "var startAt= 0;// Time of last start / resume. (0 if not running)",
	  "var lapTime= 0;// Time on the clock when last stopped in milliseconds",
	  "var now= function() {",
	  "return (new Date()).getTime();",
	  "}; ",
	  "this.start = function() {",
	  "startAt= startAt ? startAt : now();",
	  "};",
	  "this.stop = function() {",
	  "lapTime= startAt ? lapTime + now() - startAt : lapTime;",
	  "startAt= 0; // Paused",
	  "};",
	  "this.reset = function() {",
	  "lapTime = startAt = 0;",
	  "};",
	  "this.time = function() {",
	  "return lapTime + (startAt ? now() - startAt : 0); ",
	  "};"]
  },
  {
      "l":["get_amount"],
      "relation":"instance_of",
      "r":["function"],
      "params":["colorStr"],
      "lines":[
	        "var factor = 16;",
	        "colors = {'red':[255,0,0],'blue':[0,0,255],'orange':[255, 87, 51],'green':[41, 159, 55],'clear':[0, 0, 0]};",
          "if(typeof this.checker_timeouts == 'undefined') { this.checker_timeouts = {'red':null, 'blue':null, 'orange':null, 'green':null, 'clear':null}; }",
          "var check = false;",
          "if(typeof this.last == 'undefined') { this.last = {}; check = true; }",
          "if(!this.checker_timeouts[colorStr]) { this.checker_timeouts[colorStr] = new clsStopwatch(); check=true; }",
          "if(this.checker_timeouts[colorStr].time() > 1000) { check = true; }",
	        "if(check) {",
	        "    this.checker_timeouts[colorStr].reset();",
          "    this.checker_timeouts[colorStr].start();",
	        "    pixels = background.getPixels(new Phaser.Rectangle(0,0,400,300)).data;",
	        "    color = colors[colorStr];",
	        "    count = 0;",
	        "    for (var ii = 0; ii < pixels.length; ii += 4*factor){",
	        "        R = pixels[ii]; G = pixels[ii+1]; B = pixels[ii+2];",
	        "        if (color[0] == R && color[1] == G && color[2] == B){ ",
	        "            count += 1;",
	        "        }",
	        "     }",
	        "     this.last[colorStr] =  count*10.*factor / (400.*300.);",
	        "}",
	        "return this.last[colorStr];"
      ]
  }
,
